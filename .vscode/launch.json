        {
            "name": "quiz_app_frontend",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file=appsettings.debug.json"
            ]
        },
        {
            "name": "quiz_app_frontend (profile mode)",
            "request": "launch",
            "type": "dart",
